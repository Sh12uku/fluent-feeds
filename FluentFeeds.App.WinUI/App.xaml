<Application
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:converters="using:FluentFeeds.App.WinUI.Converters"
	x:Class="FluentFeeds.App.WinUI.App">
	
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
			</ResourceDictionary.MergedDictionaries>

			<converters:SymbolToIconElementConverter x:Key="SymbolToIconElementConverter" />

			<converters:TimestampToStringConverter x:Key="TimestampToStringConverter" />

			<converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />

			<converters:BoolToVisibilityConverter x:Key="InvertedBoolToVisibilityConverter" IsInverted="True" />
		</ResourceDictionary>
	</Application.Resources>
	
</Application>
